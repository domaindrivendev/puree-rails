<% module_namespacing do -%>

class <%= class_name %>Form < TransientForm
<% attributes.each do |attr| -%>
  field :<%= attr.name %>, :<%= attr.type %>
<% end -%>

  attr_accessible <%= attributes.map { |a| ":#{a.name}" }.join(', ') %>

  def on_submit

    # Call into domain model here ...
  end
end

<% end -%>