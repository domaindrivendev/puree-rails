section
  header
    h2 <%= class_name.pluralize.humanize %>
  table
    thead
      tr
<%- attributes.each do |attr| -%>
        th <%= attr.name.humanize %>
<%- end %>
        th
    tbody
      - @<%= plural_file_name %>.each do |<%= file_name %>|
        tr
<%- attributes.each do |attr| -%>
          td = <%= file_name %>.<%= attr.name %>
<%- end %>
          td
            = link_to 'View', <%= file_name %>
  ul.actions
    li
      = link_to 'New', new_<%= file_name %>_path