section
  header
    h2 New <%= class_name.humanize %>
  = form_for @new_<%= file_name %>_form, url: <%= plural_file_name %>_path do |f|
    ol.fields
    <%- attributes.drop(1).each do |attr| -%>
      li
        = f.label :<%= attr.name %>
        = f.text_field :<%= attr.name %>
    <%- end -%>
    ul.actions
      li
        = f.submit 'Create'